{
    "name": "Sales Robot",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_title": "pop up"
    },

    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["popup.js"]
        }
    ],
    "icons":{
        "128":"logo.png"
    }
}